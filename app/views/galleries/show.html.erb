

<section id="portfolio">
      <div id="portfolio-single" class="section-inner">

		<div id="custom-content" class="white-popup-block">

			<div class="row">
			  <div class="col-sm-12 text-center">
		      <div class="owl-carousel owl-theme portfolio-slider">
		      <% @gallery.assets.each do |asset| %>
								<div class="item">
		            
							
									<%= image_tag asset.image.url(:original) %>

								</div>
		            <% end %>
		            		<div class="item">
		            			<a class="owl-video" href="http://vimeo.com/100785832"></a>
		            		</div>
		            		<div class="item">
		            			<a class="owl-video" href="http://www.youtube.com/watch?v=L53gjP-TtGE"></a>
		            		</div>
		            		</div>
		       </div><!-- .col -->
				
			<script type= 'text/javascript' >
              $(".owl-carousel").owlCarousel();
            </script>
			</div><!-- .row -->

			<div class="popup-content">
				<div class="row">

					<div class="col-sm-12">

						<div class="popup-header">
							<h2 class="popup-title"><%= @gallery.name %></h2>
							<div class="popup-subtitle">
								<%= @gallery.sub %>
							</div>
							<div class="popup-line"></div>
						</div>

					</div>

				</div>

				<div class="row">

					<div class="col-sm-12">
						<%= @gallery.description.html_safe %>
					</div>

				</div>

			</div>

			<section class="popup-callout">
				<div class="row">

					<div class="col-sm-12">
						<h3>Like Our Works?</h3>
						<div class="module-line"></div>
					</div>

					<div class="col-sm-12">
						<div class="btn-list">
							<a href="index.html#services" class="btn btn-custom-3" target="_blank">View Services</a>
							<a href="index.html#contact" class="btn btn-custom-4" target="_blank">Start Project</a>
						</div>
					</div>

				</div>
			</section>

		</div>


    </div> <!-- END #portfolio-single .section-inner -->
</section> <!-- END SECTION #portfolio-->